{
  "name": "pigeon-map-landing",
  "version": "0.2.6",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 3002",
    "build": "node scripts/interactive-build.js",
    "build:auto": "npm run version:inject && next build",
    "build:clean": "next build",
    "start": "next start",
    "lint": "next lint",
    "version": "node scripts/version-manager.js patch",
    "version:minor": "node scripts/version-manager.js minor",
    "version:major": "node scripts/version-manager.js major",
    "version:build": "npm run version && npm run build",
    "version:build:minor": "npm run version:minor && npm run build",
    "version:build:major": "npm run version:major && npm run build",
    "version:deploy": "npm run version && npm run build && npm run deploy",
    "version:deploy:minor": "npm run version:minor && npm run build && npm run deploy",
    "version:deploy:major": "npm run version:major && npm run build && npm run deploy",
    "version:inject": "node -e \"const vm=require('./scripts/version-manager.js');const pkg=require('./package.json');vm.updateDashboardPWA(pkg.version);vm.updateNextConfig(pkg.version);\"",
    "version:status": "node scripts/version-status.js",
    "version:watch": "node scripts/version-status.js --watch",
    "build:demo": "./scripts/demo-interactive-build.sh",
    "deploy:clean": "./scripts/deploy-to-plesk.sh nodejs && npm run releases:latest",
    "deploy": "node scripts/interactive-deploy.js",
    "deploy:static": "./scripts/deploy-to-plesk.sh static",
    "deploy:nodejs": "./scripts/deploy-to-plesk.sh nodejs",
    "releases": "./scripts/manage-releases.sh list",
    "releases:latest": "./scripts/manage-releases.sh latest",
    "releases:clean": "./scripts/manage-releases.sh clean",
    "logs": "./scripts/view-logs.sh",
    "logs:blog": "./scripts/view-logs.sh blog.log",
    "logs:app": "./scripts/view-logs.sh app.log",
    "logs:today": "./scripts/manage-logs.sh today",
    "logs:list": "./scripts/manage-logs.sh list",
    "logs:size": "./scripts/manage-logs.sh size",
    "logs:clean": "./scripts/manage-logs.sh clean",
    "logs:tail": "./scripts/manage-logs.sh tail",
    "logs:clear": "rm -rf logs/*.log",
    "prepack": "npm run build",
    "package": "npm run deploy"
  },
  "dependencies": {
    "@react-oauth/google": "^0.12.2",
    "@tailwindcss/typography": "^0.5.16",
    "axios": "^1.9.0",
    "clsx": "^2.1.1",
    "gray-matter": "^4.0.3",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.513.0",
    "next": "15.3.3",
    "next-intl": "^4.1.0",
    "next-pwa": "^5.6.0",
    "next-sitemap": "^4.2.3",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-phone-input-2": "^2.15.1",
    "rehype-raw": "^7.0.0",
    "rehype-stringify": "^10.0.1",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-html": "^16.0.1",
    "remark-rehype": "^11.1.2",
    "zod": "^3.25.56"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/js-cookie": "^3.0.6",
    "@types/minimatch": "^6.0.0",
    "@types/next-pwa": "^5.6.9",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "postcss": "^8.5.4",
    "tailwindcss": "^4.1.8",
    "typescript": "^5"
  }
}
